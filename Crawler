    // - MAIN MENU -
    function mainMenu(x){
        if(x=="N"){
            if(dropcarrier==0){}else{
                dropcarrier=dropcarrier-1
            }
        }else if(x=="S"){
            if(dropcarrier!=7){
                x = mainMenuStorage[dropcarrier+1]
                if(x==undefined){}else{
                    dropcarrier=dropcarrier+1
                }
            }else{}
        }
        resetSideView()
        g = 1
        for(let i = 0; i < 9; i++){
            x = mainMenuStorage[i]
            y = "sideFeed"+g
            g = g+1
            if(x!=undefined){
                if(g-2==dropcarrier){
                    document.getElementById(y).innerHTML = "> " + x
                    document.getElementById(y).style.color = "yellow"
                }else{
                    document.getElementById(y).innerHTML = "◬ " + x
                }
            }else{
                document.getElementById(y).innerHTML = "&nbsp"
            }
        }
    }

    function mainMenuConfirm(){
        if(dropcarrier==0){
            //PLAY DEMO
            document.getElementById("feed").innerHTML = "LOADING COMPLETE"
            displayCoords()
            setTimeout(function() {
                writeWorldMap()
                writeWorldTiles()
                displayCoords()
                checkBiome()
                printRoom()
                menuMode="overworld"
            }, 1)
            setTimeout(function() {
                writeInv()
                coord.x = spawnCoord.x
                coord.y = spawnCoord.y
                document.getElementById("feed").innerHTML = "PRESS '5' OR 'SPACE' TO START"
            }, 2)
            dropcarrier=0
        }else if(dropcarrier==1){
            //OPTIONS
            x="X"
            dropcarrier=0
            mainMenu(x)
            document.getElementById("feed").innerHTML = "WIP"
        }else if(dropcarrier==2){
            //CREDITS
            x="X"
            dropcarrier=0
            mainMenu(x)
            document.getElementById("feed").innerHTML = "Made pretty much entirely by Deane Barberio using Javascript & HTML."
        }else if(dropcarrier==3){
            //QUIT
            x="X"
            dropcarrier=0
            mainMenu(x)
            document.getElementById("feed").innerHTML = "You are gonna need to do that manually for now."
        }else if(dropcarrier==4){
            //UNSET BUFFER
        }
    }

    // - VIEW OBJECT -
    function viewTile(x,y){
        x=coord.x+x
        y=coord.y+y
        z = tilesList[coord.z][x][y] //NOTE: The planned order of operations is: Wall Tiles, Entities(Player, allies, enemies, etc.), Items, & Tiles. Currently, entities are not included.
        a = tileIDs.find(y => y.id === z)
        if(a.wall != 0){b=a.name}else{ //Checks if tile x is a wall. If it is, it is used for writing.
            z = itemsList[coord.z][x][y]
            if(z == "empty" || z == undefined){ //Checks if x is empty. If it is not, it is used for writing.    //NOTE: Entities are supposed to go before this when they are implemented in full. Both ground items & entities will be visible on the same tile.
                z = tilesList[coord.z][x][y]
                a = tileIDs.find(y => y.id === z)
                b = a.name
            }else if(z[0]=="scint"){
                b = z[1]+" Scint"
            }else{
                a = z[0]
                a = itemIDs.find(y => y.id === a)
                b = a.name
            } 
        }
        document.getElementById("view").innerHTML=b + "<br>" + a.desc
    }

    // - MOVEMENT BUTTONS CORE -
    function moveUp(){
        x = tilesList[coord.z][coord.x][coord.y]
        var y = tileIDs.find(y => y.id === x)
        coord.z = coord.z + 1;
        playerCoord.z = playerCoord.z + 1
        if(y.stair == 2 || y.stair == 3){ // stair 2 means the stairs move up. stair 3 means the stairs move both up and down.
            displayCoords();
            checkBiome()
            document.getElementById("feed").innerHTML = "You move up the " + y.name + "."
            coord.z = coord.z + 1
            checkFloor()
        } else if(y.stair == 0 || y.stair == 1){ //stair 0 means the area is solid and inpassable. stair 1 means the stairs move down.
            coord.z = coord.z - 1
            playerCoord.z = playerCoord.z - 1
            document.getElementById("feed").innerHTML = "There is nowhere to move up!" 
        } else {
            coord.z = coord.z - 1
            playerCoord.z = playerCoord.z - 1
            document.getElementById("feed").innerHTML = "ERROR: " + y.name + " Lacks a stair value. Assuming stair = 0."
        }
        printRoom();
        endTurn();
    }
    function moveNorth(){
        coord.y = coord.y + 1;
        playerCoord.y = playerCoord.y + 1;
        x = tilesList[coord.z][coord.x][coord.y]
        var y = tileIDs.find(y => y.id === x)
        if(y.wall == 0){ // wall 0 means the area is open.
            displayCoords();
            checkBiome()
            document.getElementById("feed").innerHTML = "You move North."
        } else if(y.wall == 1){ //wall 1 means the area is solid and inpassable.
            coord.y = coord.y - 1
            playerCoord.y = playerCoord.y - 1;
            document.getElementById("feed").innerHTML = "You try to move North, but there is a " + y.name + " in the way!" 
        } else {
            coord.y = coord.y - 1
            playerCoord.y = playerCoord.y - 1;
            document.getElementById("feed").innerHTML = "ERROR: " + y.name + " Lacks a wall value. Assuming wall = 1."
        }
        printRoom();
        endTurn();
    }
    function moveDown(){
        x = tilesList[coord.z][coord.x][coord.y]
        var y = tileIDs.find(y => y.id === x)
        coord.z = coord.z - 1;
        playerCoord.z = playerCoord.z - 1
        if(coord.z<0){ //If the player tries to go below 0, they are asked if they want to leave.      //NOTE: You cannot leave just yet.
            document.getElementById("feed").innerHTML = "Would you like to leave?"
            a=["YES","NO"]
            coord.z = coord.z + 1
            playerCoord.z = playerCoord.z + 1;
            document.getElementById("feed").innerHTML += "<br>NOTE: YOU CANNOT LEAVE AS OF YET." //PLACEHOLDER
        }else if(y.stair == 1 || y.stair == 3){ // stair 1 means the stairs move down. stair 3 means the stairs move both up and down.
            displayCoords();
            checkBiome()
            document.getElementById("feed").innerHTML = "You move down the " + y.name + "."
            coord.z = coord.z - 1
            checkFloor()
            printRoom();
        } else if(y.stair == 0 || y.stair == 2){ //stair 0 means the area is solid and inpassable. stair 2 means the stairs move up.
            coord.z = coord.z + 1
            playerCoord.z = playerCoord.z + 1;
            document.getElementById("feed").innerHTML = "There is nowhere to move down!" 
            printRoom();
        } else {
            coord.z = coord.z + 1
            playerCoord.z = playerCoord.z + 1;
            document.getElementById("feed").innerHTML = "ERROR: " + y.name + " Lacks a stair value. Assuming stair = 0."
            printRoom();
        }
    }
    function moveWest(){
        coord.x = coord.x - 1;
        playerCoord.x = playerCoord.x - 1;
        x = tilesList[coord.z][coord.x][coord.y]
        var y = tileIDs.find(y => y.id === x)
        if(y.wall == 0){ // wall 0 means the area is open.
            displayCoords();
            checkBiome()
            document.getElementById("feed").innerHTML = "You move West."
