    function displayCoords(){ // Sets coord to current coordinates, and updates it.
        document.getElementById("coord").innerHTML = "Location : " + (coord.x) + ", " + coord.y + ", " + coord.z;
    }
    displayCoords()
    setTimeout(function() {
