        break;
    case "7": x = "NW"; if(menuMode=="main"){}else if(menuMode=="overworld"){moveNW()}else if(menuMode=="options"){}else if(menuMode=="inv"){}else if(menuMode=="self"){}else if(menuMode=="mind"){}else if(menuMode=="pickup"){pickupItem(x)}else if(menuMode=="drop"){dropItem(x)}else if(menuMode=="throw"){throwItem(x)}
