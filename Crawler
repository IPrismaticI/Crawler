    case "v":
        break;
    case "b":
        break;
    case "n": writeMind()
        break;
    case "m":
        break;
    case ",":
        break;
    case ".":
        break;
    case "/":
        break;
    case "ArrowUp": moveNorth()
        break;
    case "ArrowDown": moveSouth()
        break;
    case "ArrowLeft": moveWest()
        break;
    case "ArrowRight": moveEast()
        break;
    case " ": moveWait()
        break;
    // + Shift
    case "~":
