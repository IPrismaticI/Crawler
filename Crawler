    // - WORLD GENERATOR - WIP
    const world = [];
    var worldDict = {
        0: '=', // Glacier (Edge of Stage)
        1: '.', // Ocean (Walkway)
        2: '_', // Plains (Room)
        3: '^', // Forest (Flooded Walkway)
        4: 'M', // Mountains (Flooded Room)
        5: '~', // Desert (Stairs Room)
        6: '+', // Sun-Scorned
    }

    function writeWorldTiles(){ //Used to generate the floor, items, & traps.   //currently designed to only print z-level 0.
        output = "";
        var worldheight = worldsize.y;
        var worldwidth = worldsize.x;
        for(var row = 0; row < worldheight; row++){
            tilesList.push([])
            document.getElementById("sideFeed").innerHTML += "<br>"
                for (var col = 0; col < worldwidth; col++){
                    if(col==0){
                        x = 0
                    }else{
                        x = Math.floor(col / 50)
                    }
                    if(row==0){
                        y = 0
                    }else{
                        y = Math.floor(row / 50)
                    }
                    z = world[x][y]
                    if (z == 0){
                        y = "core_stonewall1"
                    }else if (z == 1){
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }else if (z == 2){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 3){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 4){
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }else if (z == 5){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 6){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 7){
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }else{
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }
                    tilesList[row].push(y)
                    //document.getElementById("sideFeed").innerHTML += x
                }
                //document.getElementById("sideFeed").innerHTML += "<br><br>"
        }
        playerCoord.x = spawnCoord.x
        playerCoord.y = spawnCoord.y
        structureGen()
    }

    function writeWorldMap(){
        output = "";
        var worldheight = (worldsize.x / 50);
        var worldwidth = (worldsize.y / 50);
        for(var row = 0; row < worldheight; row++){ //Prints default room layouts.
            world.push([])
            output += "<div class = 'row'>"
                for (var col = 0; col < worldwidth; col++){
                    if (row >= worldheight - 1 || col >= worldwidth - 1 || row <= 0 || col <= 0){
                        world[row].push(0)
                        output += worldDict[world[row][col]];
                    }else{ //NOTE: Put special dungeon layouts here in the future.
                        world[row].push(1)
                        output += worldDict[world[row][col]];
                    }
                }
        }
        x = Math.floor((Math.random() * (worldheight - 2)) + 1)
        y = Math.floor((Math.random() * (worldwidth - 2)) + 1)
        world[x][y] = 5 //Sets a random room as a stairs room. NOTE: filter to check player coords and set that as the stair level.
        roomsVal = Math.floor((Math.random() * 4) + 2)
        while (roomsVal != 0){
            x = Math.floor((Math.random() * (worldheight - 2)) + 1)
            y = Math.floor((Math.random() * (worldwidth - 2)) + 1)
            z = world[x][y]
            if (z == 1){
                world[x][y] = 2
                roomsVal = roomsVal - 1
            } else if (z == 3){
                world[x][y] = 4
                roomsVal = roomsVal - 1
            } else if (z == 2 || z == 4 || z == 5){ //Detects if z is already a room.
            }
        document.getElementById('sideFeed').innerHTML = output;
        }
    }

    // - TILE GENERATORS & RECALLERS -
    function printRoom(){
        var x = tilesList[coord.x][coord.y] //Retrieves space by coords.
        var y = tileIDs.find(y => y.id === x)
        document.getElementById("area").innerHTML = biome.cur; // Prints biome name.
        document.getElementById("feed").innerHTML += "<br><br>" + y.desc // Print room desc.
        writeMap()
    }

    function structureGen(){ //Pregenerates rooms & halls.
        var worldheight = (worldsize.x / 50);
        var worldwidth = (worldsize.y / 50);
        for(var row = 1; row < worldheight - 1; row++){
                for (var col = 1; col < worldwidth - 1; col++){
                    z = world[col][row]
                    if (z == 1){
                        x = structureListA
                    }else{ //NOTE: Put special room & hall designs here in the future, e.g flooded, burning.
                        x = structureListA
                    }
                    y = Math.floor(Math.random() * x.length)
                    x = x[y]
                    y = x.length
                    for (var build = 0; build != y; build++){
                        a = x[build][0]
                        b = x[build][1]
                        c = row * 50
                        d = col * 50
                        a = a + c
                        b = b + d
                        tilesList[a][b] = x[build][2]
                        
                    }
                }
        }
        var structureSeeker = 0
        var x = structureListA.length
        while (structureSeeker != x){
            var structureBuilder = 0
            coord.x = structureListA[structureSeeker][0]
            coord.y = structureListA[structureSeeker][1]
            y = structureBP[structureSeeker].length
            while (structureBuilder != y){
                a = coord.x + structureBP[structureSeeker][structureBuilder][0]
                b = coord.y + structureBP[structureSeeker][structureBuilder][1]
                tilesList[a][b] = structureBP[structureSeeker][structureBuilder][2]
                structureBuilder = structureBuilder + 1
            }
            structureSeeker = structureSeeker + 1
        }
        coord.x = spawnCoord.x
        coord.y = spawnCoord.y
    }

