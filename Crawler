    function moveWait(){
        document.getElementById("feed").innerHTML = "You stay put."
        displayCoords();
        checkBiome();
        printRoom();
    }
    
    // debug teleport
    function movePoof(){
        x = prompt("X coord")
        y = prompt("Y coord")
        z = prompt("Z coord")
        coord.x = parseInt(x)
        coord.y = parseInt(y)
        coord.z = parseInt(z)
        playerCoord.x = parseInt(x)
        playerCoord.y = parseInt(y)
        playerCoord.z = parseInt(z)
        moveWait()
        document.getElementById("feed").innerHTML = "WOOOOSH!!"
    }

    // - ITEM INTERACTION -
    // ITEM PICKUP MENU
    function menuPickup(){
        document.getElementById("feed").innerHTML = "Pick up where?<br>(Use arrow keys or numpad.)"
    }
    // ITEM PICKUP PROCESSOR
    function pickupItem(x){
        y = 1
        if(x=="SW"){
            coord.x=coord.x-1
            coord.y=coord.y-1
        }else if(x=="S"){
            coord.y=coord.y-1
        }else if(x=="SE"){
            coord.x=coord.x+1
            coord.y=coord.y-1
        }else if(x=="W"){
            coord.x=coord.x-1
        }else if(x=="C"){
        }else if(x=="E"){
            coord.x=coord.x+1
        }else if(x=="NW"){
            coord.x=coord.x-1
            coord.y=coord.y+1
        }else if(x=="N"){
            coord.y=coord.y+1
        }else if(x=="NE"){
            coord.x=coord.x+1
            coord.y=coord.y+1
        }else{
            document.getElementById("feed").innerHTML = "What was I doing again?<br>(ERROR: Pickup Direction Invalid. Cancelling.)"
            y = 0
        }
        if(y==1){
            x = itemsList[coord.x][coord.y]
            itemsList[coord.x][coord.y] = "empty"
            if(Array.isArray(x)){
                if(x[0]=="scint"){
                    scint = scint + x[1]
                    document.getElementById("feed").innerHTML = "Picked Up : " + x[1] + " Scint"
                }else{
                    invStorage.push(x)
                    z = itemIDs.find(y => y.id === x[0])
                    document.getElementById("feed").innerHTML = "Picked Up : " + z.name
                }                
            }else if(x=="empty"){
                document.getElementById("feed").innerHTML = "There is nothing here to pick up!"
            }else{
                invStorage.push(x)
                z = itemIDs.find(y => y.id === x)
                document.getElementById("feed").innerHTML = "Picked Up : " + z.name
            }
        }
        coord.x=playerCoord.x
        coord.y=playerCoord.y
        menuMode="overworld"
        writeMap()
        writeInv()
    }

