    // debug teleport
    function movePoof(){
        x = prompt("X coord")
        y = prompt("Y coord")
        z = prompt("Z coord")
        coord.x = parseInt(x)
        coord.y = parseInt(y)
        coord.z = parseInt(z)
        playerCoord.x = parseInt(x)
        playerCoord.y = parseInt(y)
        playerCoord.z = parseInt(z)
        moveWait()
        document.getElementById("feed").innerHTML = "WOOOOSH!!"
    }

    // - ITEM INTERACTION -
    // ITEM PICKUP MENU
    function menuPickup(){
        document.getElementById("feed").innerHTML = "Pick up where?<br>(Use arrow keys or numpad.)"
    }
    // ITEM PICKUP PROCESSOR
    function pickupItem(x){
        y = 1
        if(x=="SW"){
            coord.x=coord.x-1
            coord.y=coord.y-1
        }else if(x=="S"){
            coord.y=coord.y-1
        }else if(x=="SE"){
            coord.x=coord.x+1
            coord.y=coord.y-1
        }else if(x=="W"){
            coord.x=coord.x-1
        }else if(x=="C"){
        }else if(x=="E"){
            coord.x=coord.x+1
        }else if(x=="NW"){
            coord.x=coord.x-1
            coord.y=coord.y+1
        }else if(x=="N"){
            coord.y=coord.y+1
        }else if(x=="NE"){
            coord.x=coord.x+1
            coord.y=coord.y+1
        }else{
            document.getElementById("feed").innerHTML = "What was I doing again?<br>(ERROR: Pickup Direction Invalid. Cancelling.)"
            y = 0
        }
        if(y==1){
            x = itemsList[coord.z][coord.x][coord.y]
            if(x[0]=="scint"){
                scint = scint + x[1]
                document.getElementById("feed").innerHTML = "Picked Up : " + x[1] + " Scint"
                itemsList[coord.z][coord.x][coord.y] = "empty"   
            }else if(x=="empty"||x==undefined){
                document.getElementById("feed").innerHTML = "There is nothing here to pick up!"
            }else if(invStorage.length >= invMax){
                z = itemIDs.find(y => y.id === x[0])
                document.getElementById("feed").innerHTML = "You tried to pick up the " + z.name + " but your inventory was too full!"
            }else{
                invStorage.push(x)
                z = itemIDs.find(y => y.id === x[0])
                document.getElementById("feed").innerHTML = "Picked Up : " + z.name
                itemsList[coord.z][coord.x][coord.y] = "empty"
            }
        }
        coord.x=playerCoord.x
        coord.y=playerCoord.y
        menuMode="overworld"
        writeMap()
        writeInv()
    }

    // ITEM DROP
    function menuDrop(){
        if(invStorage.length==0){ //NOTE: When scint dropping gets added, have another sensor that skips item dropping if the player has scint but no items.
            document.getElementById("feed").innerHTML = "You have nothing to drop!"
            menuMode="overworld"
        }else{
            document.getElementById("feed").innerHTML = "Drop what?<br>(Use arrow keys or numpad to select. Press 5 or SPACE to confirm.)"
            dropcarrier=0
            x="X"
            dropItem(x)
        }
    }
    function dropItem(x){
        if(x=="N"){
            if(dropcarrier==0){}else{
                dropcarrier=dropcarrier-1
            }
        }else if(x=="S"){
            x = invStorage[dropcarrier+1+(invPage * 8)]
            if(dropcarrier!=7&&Array.isArray(x)){dropcarrier=dropcarrier+1
            }else{}
        }
        resetSideView()
        document.getElementById("sideFeed1").innerHTML = " -- Inventory --<br><br>Items : " + invStorage.length + "&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp" + "Scint : " + scint
        g = 2
        for(let i = 8 * invPage; i < (8 * invPage) + 8; i++){
            x = invStorage[i]
            y = "sideFeed"+g
            g = g+1
            if(Array.isArray(x)){
                x = itemIDs.find(y => y.id === x[0])
                if(g-3==dropcarrier){
                    document.getElementById(y).innerHTML = "> " + x.name
                    document.getElementById(y).style.color = "yellow"
                }else{
                    document.getElementById(y).innerHTML = x.symbol + " " + x.name
                }
            }else{
                document.getElementById(y).innerHTML = "&nbsp"
            }
        document.getElementById("sideFeed0").innerHTML = "Page : " + (invPage + 1)
        }
        if(invPage > 0){
            document.getElementById("sideButton1").onclick = turnInvPageLeftDrop
            document.getElementById("sideButton1").style = "border-color:white;color:white"
        }
        if(invPage < ((invMax / 8)-1)){
            document.getElementById("sideButton2").onclick = turnInvPageRightDrop
            document.getElementById("sideButton2").style = "border-color:white;color:white"
        }
    }
    function turnInvPageLeftDrop(){
        invPage = invPage-1;
        dropcarrier=0
        dropItem()
    }
    function turnInvPageRightDrop(){
        invPage = invPage+1;
        dropcarrier=0
        dropItem()
    }
    function dropItemConfirm(){
        z = invStorage[dropcarrier]
        y = itemsList[coord.z][coord.x][coord.y]
        x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
        if(x!=0||y!="empty"){
            coord.y=coord.y+1
            y = itemsList[coord.z][coord.x][coord.y]
            x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
            if(x!=0||y!="empty"){
                coord.x=coord.x-1
                y = itemsList[coord.z][coord.x][coord.y]
                x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                if(x!=0||y!="empty"){
                    coord.y=coord.y-1
                    y = itemsList[coord.z][coord.x][coord.y]
                    x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                    if(x!=0||y!="empty"){
                        coord.y=coord.y-1
                        y = itemsList[coord.z][coord.x][coord.y]
                        x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                        if(x!=0||y!="empty"){
                            coord.x=coord.x+1
                            y = itemsList[coord.z][coord.x][coord.y]
                            x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                            if(x!=0||y!="empty"){
                                coord.x=coord.x+1
                                y = itemsList[coord.z][coord.x][coord.y]
                                x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                                if(x!=0||y!="empty"){
                                    coord.y=coord.y+1
                                    y = itemsList[coord.z][coord.x][coord.y]
                                    x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                                    if(x!=0||y!="empty"){
                                        coord.y=coord.y+1
                                        y = itemsList[coord.z][coord.x][coord.y]
                                        x = tileIDs.find(y => y.id === tilesList[coord.z][coord.x][coord.y]).wall
                                        if(x!=0||y!="empty"){
                                            document.getElementById("feed").innerHTML = "There is nowhere to drop your " + itemIDs.find(y => y.id === z[0]).name + "!"
                                            y=0
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        if(y==0){}else{
            itemsList[coord.z][coord.x][coord.y] = z
            invStorage.splice(dropcarrier, 1)
            document.getElementById("feed").innerHTML = "You drop your " + itemIDs.find(y => y.id === z[0]).name + "."
        }
        coord.x=playerCoord.x
        coord.y=playerCoord.y
        menuMode="overworld"
        dropcarrier=0
        writeMap()
        writeInv()
    }
