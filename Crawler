    }

    // - WORLD GENERATOR - WIP
    const world = [];
    var worldDict = {
        0: '=', // Glacier (Edge of Stage)
        1: '.', // Ocean (Basic)
        2: '_', // Plains (Flooded)
        3: '^', // Forest (UNSET)
        4: 'M', // Mountains (UNSET)
        5: '~', // Desert (UNSET)
        6: '+', // Sun-Scorned (UNSET)
    }

    function writeWorldTiles(){ //Used to generate the floor, walls, & traps.   //currently designed to only print z-level 0.
        output = "";
        var worldheight = worldsize.y;
        var worldwidth = worldsize.x;
        for(var row = 0; row < worldheight; row++){
            tilesList[coord.z].push([])
                for (var col = 0; col < worldwidth; col++){
                    if(col==0){
                        x = 0
                    }else{
                        x = Math.floor(col / 50)
                    }
                    if(row==0){
                        y = 0
                    }else{
                        y = Math.floor(row / 50)
                    }
                    z = world[x][y]
                    if (z == 0){
                        y = "core_stonewall1"
                    }else if (z == 1){
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }else if (z == 2){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 3){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 4){
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }else if (z == 5){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 6){
                        x = Math.floor(Math.random() * tiletable1.length);
                        y = tiletable1[x]
                    }else if (z == 7){
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }else{
                        x = Math.floor(Math.random() * tiletable2.length);
                        y = tiletable2[x]
                    }
                    tilesList[coord.z][row].push(y)
                    //document.getElementById("sideFeed1").innerHTML += x
                }
                //document.getElementById("sideFeed1").innerHTML += "<br><br>"
        }
        playerCoord.x = spawnCoord.x
        playerCoord.y = spawnCoord.y
        structureGen()
    }

    function writeWorldMap(){
        output = "";
        var worldheight = (worldsize.x / 50);
        var worldwidth = (worldsize.y / 50);
        for(var row = 0; row < worldheight; row++){ //Prints default room layouts.
            world.push([])
            output += "<div class = 'row'>"
                for (var col = 0; col < worldwidth; col++){
                    if (row >= worldheight - 1 || col >= worldwidth - 1 || row <= 0 || col <= 0){
                        world[row].push(0)
                        output += worldDict[world[row][col]];
                    }else{ //NOTE: Put special dungeon layouts here in the future.
                        world[row].push(1)
                        output += worldDict[world[row][col]];
                    }
                }
        }
    }

    // - TILE GENERATORS & RECALLERS -
    function checkFloor(){
        if(floorList[coord.z] == false){
            writeWorldMap()
            writeWorldTiles()
            printRoom()
        }
    }

    function printRoom(){
        var x = tilesList[coord.z][coord.x][coord.y] //Retrieves space by coords.
        var y = tileIDs.find(y => y.id === x)
        document.getElementById("area").innerHTML = biome.cur; // Prints biome name.
        document.getElementById("feed").innerHTML += "<br><br>Below you is:<br><br>" + y.name + " : " + y.desc // Print room desc.
        x = itemsList[coord.z][coord.x][coord.y]
        if(x != "empty"){
            y = itemIDs.find(y => y.id === x[0])
            if(x[0]=="scint"){
                document.getElementById("feed").innerHTML += "<br><br>" + x[1] + " " + y.name + " : " + y.desc
            }else{
                document.getElementById("feed").innerHTML += "<br><br>" + y.name + " : " + y.desc
            }
        }
        writeMap()
    }

    function structureGen(){ //Pregenerates rooms & halls.
        var worldheight = (worldsize.x / 50);
        var worldwidth = (worldsize.y / 50);
        for(var row = 1; row < worldheight - 1; row++){
                for (var col = 1; col < worldwidth - 1; col++){
                    z = world[col][row]
                    if (z == 1){ //Basic
                        x = structureListA
                    }else{ //NOTE: Put special room & hall designs here in the future, e.g flooded, burning.
                        x = structureListA
                    }
                    y = Math.floor(Math.random() * x.length)
                    x = x[y]
                    for (var buildrow = 0; buildrow < 50; buildrow++){
                        for (var buildcol = 0; buildcol < 50; buildcol++){
                            a = row * 50
                            b = col * 50
                            a = buildrow + a
                            b = buildcol + b
                            carrier = x[buildcol][buildrow]
                            if(carrier == "0"){ //Detects tile as Empty.
                            }else if(carrier == "1"){ //Palette-based Wall        //NOTE: Set this and most others to place a tile based on a central palette. Preferably check this earlier on in the for loop rather than on every placement.
                                tilesList[coord.z][a][b] = "core_stonewall1"
                            }else if(carrier == "2"){ //Palette-based Fluid
                                tilesList[coord.z][a][b] = carrier //TODO
                            }else if(carrier == "3"){ //Palette-based Pit
                                tilesList[coord.z][a][b] = carrier //TODO
                            }else if(carrier == "4"){ //UNSET
                                tilesList[coord.z][a][b] = carrier
                            }else if(carrier == "5"){ //UNSET
                                tilesList[coord.z][a][b] = carrier
                            }else{ // For special variables.
                                tilesList[coord.z][a][b] = carrier
                            }
                        }
                    }
                }
            }
        while(a.wall != 0 || a.stair != 0){
            x = Math.floor((Math.random() * (worldsize.x-50))+50)
            y = Math.floor((Math.random() * (worldsize.y-50))+50)
            z = tilesList[coord.z][x][y]
            a = tileIDs.find(x => x.id === z)
        }
        tilesList[coord.z][x][y] = "core_stairsdown1" //NOTE: Replace this with palette-based stairs.
        spawnCoord.x = x
        spawnCoord.y = y
        while(b.wall != 0 || b.stair != 0){
            x = Math.floor((Math.random() * (worldsize.x-50))+50)
            y = Math.floor((Math.random() * (worldsize.y-50))+50)
            z = tilesList[coord.z][x][y]
            b = tileIDs.find(x => x.id === z)
        }
        tilesList[coord.z][x][y] = "core_stairsup1"
        coord.x = spawnCoord.x
        coord.y = spawnCoord.y
        itemGen()
    }

    function itemGen(){ //Generates items & sets entity list up for the floor.
        a = itemtable1 //NOTE: Base this off of the specific area when those get implemented.
        var worldheight = worldsize.y;
        var worldwidth = worldsize.x;
        for(var row = 0; row < worldheight; row++){
            itemsList[coord.z].push([])
                for (var col = 0; col < worldwidth; col++){
                    z = tilesList[coord.z][col][row]
                    z = tileIDs.find(y => y.id === z)
                    if (z.wall == 0 && z.stair == 0){
                        x = Math.floor(Math.random() * 10)
                        if(x == 0){
                            y = Math.floor(Math.random() * a.length)
                            y = a[y]
                            if(y=="scint"){
                                y = ["scint",Math.floor((Math.random() * 100) + 50)] //Heavily considering having all items be arrays so that I can easily have ephemera-infused items.
                            }else{
                                x=itemIDs.find(z => z.id === y)
                                x=x.mod
                                if(x==undefined){
                                    y=[y,x]
                                }else{
                                    b=[Math.floor(Math.random() * x.length)]
                                    y = [y,x[b]]
                                }
                            }
                        }else{
                            y = "empty"
                        }
                    }else{ //NOTE: Currently this is set up to only not print items on spaces with wall values of 1. This may need to be changed later on.
                        y = "empty"
                    }
                    itemsList[coord.z][row].push(y)
                }
        }
        playerCoord.x = spawnCoord.x
        playerCoord.y = spawnCoord.y
        floorList[coord.z] = true
    }
    
    // - MAGIC SYSTEMS -
    function viewMagic(x){
        for(let i = 0; i < 8; i++){
            x = magicStorage[i]
            y = "sideFeed"+g
            g = g+1
            if(x!=undefined){ //NOTE: Currently no spells have been implemented.
                x = spellIDs.find(y => y.id === x)
                document.getElementById(y).innerHTML = x.symbol + " " + x.name
            }else{
                document.getElementById(y).innerHTML = "&nbsp"
            }
        }
    }
    function writeMagic(x){
        if(x=="N"){
            if(dropcarrier==0){}else{
                dropcarrier=dropcarrier-1
            }
        }else if(x=="S"){
            x = invStorage[dropcarrier+1]
            if(dropcarrier!=7&&x!=undefined){dropcarrier=dropcarrier+1
            }else{}
        }
        menuMode="magic"
        resetSideView()
        document.getElementById("sideFeed1").innerHTML = " -- Arcana --"
        g = 2
        for(let i = 0; i < 8; i++){
            x = magicStorage[i]
            y = "sideFeed"+g
            g = g+1
            if(x!=undefined){ //NOTE: Currently no spells have been implemented.
                x = magicIDs.find(y => y.id === x)
                document.getElementById(y).innerHTML = x.symbol + " " + x.name
            }else{
                document.getElementById(y).innerHTML = "&nbsp"
            }
        }
    }
    function confirmMagic(){
        document.getElementById("feed").innerHTML = "IOU: several magics."
        menuMode="overworld"
        dropcarrier=0
    }

    // - TURN SYSTEM & NPC ACTIONS -
    function endTurn(){
        document.getElementById("feed").innerHTML += "<br>Turn ended!"
        if(entitiesList[coord.z].length==0){}else{
            menuMode="turn"
            x = 0
            endTurnLoop(x)
        }
    }
    function endTurnLoop(x){  //NOTE: This will trigger all the turn-end things. This mostly covers NPC actions, but it also covers things like weather, usually after the NPC's actions.
        z = entitiesList[coord.z][x] //Checks AI and runs it.
        if(z[0]=="core_testfollower"){
            turncore_testfollower(z)
        }else if(z[0]=="core_testwanderer"){
            turncore_testwanderer(z)
        }else if(z[0]=="core_testcrawler"){
            turncore_testcrawler(z)
        }else{}
        x=x+1
        if(x>=entitiesList[coord.z].length){menuMode="overworld"}else{  //NOTE: Have it so the delayed reset only occurs when the entity is within view.
            setTimeout(endTurnLoop(x),1000)
        }
    }
    // - NPC AI -                             
    function turncore_testfollower(z){ //It is supposed to wander until the player gets within 10 tiles, then it starts following them. Keeping a distance might be something possible. Archers?        //NOTE: Currently has weird movements.
        if(Math.abs(z[1]-playerCoord.x)>10&&Math.abs(z[2]-playerCoord.y)>10){ //If entity's coords minus player's coords create a number larger than 10, it uses testwanderer code.
            turncore_testcrawler(z) //NOTE: Intended to be testwanderer, but that code is broken atm.
        }else{
            //while(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)||entitiesList[coord.z].find(x => x[1] === a && x[2] === b)==undefined){
            if(z[1]-playerCoord.x==0&&z[2]-playerCoord.y>0){ //Entity is North of Player.
            a=z[1]
            b=z[2]-1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x<0&&z[2]-playerCoord.y>0){ //Entity is North-West of Player.
            a=z[1]+1
            b=z[2]-1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x<0&&z[2]-playerCoord.y==0){ //Entity is West of Player.
            a=z[1]+1
            b=z[2]
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x<0&&z[2]-playerCoord.y<0){ //Entity is South-West of Player.
            a=z[1]+1
            b=z[2]+1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x==0&&z[2]-playerCoord.y<0){ //Entity is South of Player.
            a=z[1]
            b=z[2]+1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x>0&&z[2]-playerCoord.y<0){ //Entity is South-East of Player.
            a=z[1]-1
            b=z[2]+1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x>0&&z[2]-playerCoord.y==0){ //Entity is East of Player.
            a=z[1]-1
            b=z[2]
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else if(z[1]-playerCoord.x>0&&z[2]-playerCoord.y>0){ //Entity is North-East of Player.
            a=z[1]-1
            b=z[2]-1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            }else{ //Fallback.
            a=z[1]
            b=z[2]
            }
            c=tilesList[coord.z][a][b]  //Put everything here in each direction category? Might allow for more fine-tuning in direction choices.
            c=tileIDs.find(y => y.id === c)
            if(b==playerCoord.y&&a==playerCoord.x){
            }else if(c.wall!=0||entitiesList[coord.z].find(x => x[1] === a && x[2] === b)!=undefined){
                turncore_testcrawler(z)//NOTE: Intended to be testwanderer, but that code is broken atm.
            }else{
                z[1]=a
                z[2]=b
                writeMap();
            }
        }
    }
    function turncore_testwanderer(z){ //NOTE: UNFINISHED. Prone to getting stuck. //It is supposed to wander around. Exactly how depends on how I decide to go about it, though it probably follows the walls.
        a=z[1]+1
        b=z[2]
        c=tilesList[coord.z][a][b]
        c=tileIDs.find(y => y.id === c)
        if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
            a=z[1]+1
            b=z[2]-1
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                a=z[1]
                b=z[2]-1
                c=tilesList[coord.z][a][b]
                c=tileIDs.find(y => y.id === c)
                if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                    a=z[1]-1
                    b=z[2]-1
                    c=tilesList[coord.z][a][b]
                    c=tileIDs.find(y => y.id === c)
                    if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                        a=z[1]-1
                        b=z[2]
                        c=tilesList[coord.z][a][b]
                        c=tileIDs.find(y => y.id === c)
                        if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                            a=z[1]-1
                            b=z[2]+1
                            c=tilesList[coord.z][a][b]
                            c=tileIDs.find(y => y.id === c)
                            if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                                a=z[1]
                                b=z[2]+1
                                c=tilesList[coord.z][a][b]
                                c=tileIDs.find(y => y.id === c)
                                if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                                    a=z[1]-1
                                    b=z[2]+1
                                    c=tilesList[coord.z][a][b]
                                    c=tileIDs.find(y => y.id === c)
                                    if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)){
                                        a=z[1]
                                        b=z[2]
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        z[1]=a
        z[2]=b
        writeMap();
    }
    function turncore_testcrawler(z){ //Has its x and y movements modified by -1, 0, or 1 each turn. If it hits a wall, it tries again a few times, defaulting to staying still after a few failures.
        a=z[1]+Math.floor((Math.random()*3)-1)
        b=z[2]+Math.floor((Math.random()*3)-1)
        c=tilesList[coord.z][a][b]
        c=tileIDs.find(y => y.id === c)
        if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)||entitiesList[coord.z].find(x => x[1] === a && x[2] === b)!=undefined){
            a=z[1]+Math.floor((Math.random()*3)-1)
            b=z[2]+Math.floor((Math.random()*3)-1)
            c=tilesList[coord.z][a][b]
            c=tileIDs.find(y => y.id === c)
            if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)||entitiesList[coord.z].find(x => x[1] === a && x[2] === b)!=undefined){
                a=z[1]+Math.floor((Math.random()*3)-1)
                b=z[2]+Math.floor((Math.random()*3)-1)
                c=tilesList[coord.z][a][b]
                c=tileIDs.find(y => y.id === c)
                if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)||entitiesList[coord.z].find(x => x[1] === a && x[2] === b)!=undefined){
                    a=z[1]+Math.floor((Math.random()*3)-1)
                    b=z[2]+Math.floor((Math.random()*3)-1)
                    c=tilesList[coord.z][a][b]
                    c=tileIDs.find(y => y.id === c)
                    if(c.wall!=0||(b==playerCoord.y&&a==playerCoord.x)||entitiesList[coord.z].find(x => x[1] === a && x[2] === b)!=undefined){
                        a=z[1]
                        b=z[2]
                    }
                }
            }
        }
        z[1]=a
        z[2]=b
        writeMap();
    }

    function debugSummon(){     //DEBUG: Summons an entity.
        x = prompt("X coord")
        y = prompt("Y coord")
        z = prompt("Entity ID (Case Sensitive)")
        b = [z,parseInt(x),parseInt(y)]
        entitiesList[coord.z].push(b)
        a=entityIDs.find(y => y[0] === z)
        document.getElementById("feed").innerHTML = "Summoned a " + a[1]
        writeMap();
    }

    // - OBJECT CARRIERS - 
    const floorList = [false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false,false]
    const tilesList = [[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[]]
    const itemsList = [[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[]]
        //ENTITIES LIST GUIDE: 0 = ID, 1 = x coord, 2 = y coord, 3 = health
    const entitiesList = [[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[]]

    // - TILE LISTS -
    const tiletable1 = ["core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass1", "core_grass2", "core_grass3"]
